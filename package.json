{
  "name": "@webbycrown/layout-suite",
  "version": "1.0.0",
  "type": "module",
  "description": "A professional package of reusable layout components for Next.js websites built with Tailwind CSS",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.esm.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "react",
    "tailwindcss",
    "layout",
    "components",
    "ui-components"
  ],
  "author": "WebbyCrown",
  "license": "MIT",
  "peerDependencies": {
    "next": "^14.0.0 || ^15.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "rollup": "^4.18.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.4.4"
  }
}
